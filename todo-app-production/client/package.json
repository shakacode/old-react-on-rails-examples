{
  "name": "react-on-rails-pro",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "7.4.0",
    "yarn": "0.19.1"
  },
  "homepage": "https://github.com/shakacode/react-on-rails-pro",
  "repository": {
    "type": "git",
    "url": "https://github.com/shakacode/react-on-rails-pro.git"
  },
  "bugs": {
    "url": "https://github.com/shakacode/react-on-rails-pro/issues"
  },
  "scripts": {
    "build:all:prod": "yarn run build:client-bundles:prod & yarn run build:server-bundle:prod; wait",
    "build:all:rspec": "yarn run build:client-bundles:rspec & yarn run build:server-bundle:rspec; wait",
    "build:all:rspec:watch": "yarn run build:client-bundles:rspec:watch & yarn run build:server-bundle:rspec:watch; wait",
    "build:client-bundles:prod": "scripts/build:client-bundles:prod",
    "build:client-bundles:rspec": "scripts/build:client-bundles:rspec",
    "build:client-bundles:rspec:watch": "yarn run build:client-bundles:rspec -- --watch",
    "build:server-bundle:prod": "scripts/build:server-bundle:prod",
    "build:server-bundle:dev": "NODE_ENV=dev webpack --env.serverBundleDev --config webpack.config.babel.js",
    "build:server-bundle:dev:watch": "yarn run build:server-bundle:dev -- --watch",
    "build:server-bundle:rspec": "scripts/build:server-bundle:rspec",
    "build:server-bundle:rspec:watch": "yarn run build:server-bundle:rspec -- --watch",
    "check": "yarn run flow && yarn run test && yarn run lint",
    "flow": "flow",
    "flow:all": "flow --show-all-errors",
    "flow:update": "flow-typed update",
    "lint": "eslint --ext .js,.jsx .",
    "start": "NODE_ENV=dev babel-node server.js",
    "storybook": "NODE_ENV=test start-storybook -p 9001 -c .storybook",
    "test": "NODE_PATH=.:./app:./app/bundles NODE_ENV=test mocha --opts mocha.opts 'app/**/*.spec.@(js|jsx)' 'webpack-helpers/**/*.spec.@(js|jsx)'",
    "test:watch": "yarn run test -- --watch"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.20.0",
    "bootstrap-loader": "2.0.0-beta.18",
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.1",
    "es5-shim": "^4.5.8",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "immutable": "^3.8.1",
    "imports-loader": "^0.7.0",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^2.2.3",
    "jquery-ujs": "^1.1.0-1",
    "lodash": "^4.17.4",
    "lodash-uuid": "^0.0.3",
    "mirror-creator": "^1.1.0",
    "node-sass": "3.13.0",
    "normalizr": "^2.2.1",
    "postcss-loader": "^1.2.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-on-rails": "6.4.0",
    "react-redux": "^5.0.1",
    "react-router": "^4.0.0-beta.6",
    "react-router-dom": "^4.0.0-beta.6",
    "recompose": "^0.20.2",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.1",
    "redux-saga": "^0.14.3",
    "reselect": "^2.5.3",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^4.1.1",
    "sass-resources-loader": "1.2.0-beta.1",
    "sass-variable-loader": "0.0.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.2.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-jest": "^18.0.0",
    "babel-plugin-flow-react-proptypes": "^0.19.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.11",
    "babel-plugin-typecheck": "3.9.0",
    "babel-preset-react-hmre": "1.1.1",
    "chai": "3.5.0",
    "chai-immutable": "1.6.0",
    "eslint": "^3.12.2",
    "eslint-config-shakacode": "^13.2.1",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-flowtype": "^2.29.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "eslint-plugin-react": "^6.8.0",
    "flow-bin": "^0.38.0",
    "flow-typed": "2.0.0",
    "iflow-immutable": "1.0.32",
    "iflow-react-router": "^1.2.1",
    "iflow-redux-logger": "1.0.15",
    "iflow-redux-thunk": "1.0.15",
    "ignore-styles": "^5.0.1",
    "jest": "^18.1.0",
    "mocha": "^3.2.0",
    "mocha-clean": "^1.0.0",
    "mocha-junit-reporter": "^1.13.0",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.2",
    "react-transform-hmr": "1.0.4",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-logger": "2.6.1",
    "webpack-dev-server": "2.2.0-rc.0"
  },
  "peerDependencies": {
    "@kadira/storybook-addons": "^1.3.0"
  }
}
